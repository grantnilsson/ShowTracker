# Database
# For Railway, this will be automatically provided by the PostgreSQL service
DATABASE_URL="postgresql://user:password@host:5432/dbname?schema=public"

# TMDB API (use TMDB_API_KEY for server-side, NEXT_PUBLIC for client-side)
TMDB_API_KEY="your_tmdb_api_key_here"
NEXT_PUBLIC_TMDB_API_KEY="your_tmdb_api_key_here"

# Node Environment
NODE_ENV="production"
